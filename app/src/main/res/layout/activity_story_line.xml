<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    xmlns:surfaceview="http://schemas.android.com/apk/res-auto"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StoryLineActivity">

    <org.rajawali3d.cardboard.RajawaliCardboardView
        android:id="@+id/rajawali_cardboardView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        surfaceview:frameRate="60.0"
        surfaceview:renderMode="RENDER_WHEN_DIRTY"/>

    <marco.io.opearlovr.OverlayView
        android:id="@+id/overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

    </marco.io.opearlovr.OverlayView>

</RelativeLayout>
